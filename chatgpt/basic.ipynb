    "def quick_sort(arr):\n",
    "    if len(arr)==0:\n",
    "        return arr\n",
    "    \n",
    "    pivot=arr[0]\n",
    "    left=[e for e in arr if e<pivot]\n",
    "    mid=[e for e in arr if e==pivot]\n",
    "    right=[e for e in arr if e>pivot]\n",
    "\n",
    "    return quick_sort(left)+mid+quick_sort(right)\n",
    "\n",
    "quick_sort([1,4,2,6,3,8,10,5])"
   ]
