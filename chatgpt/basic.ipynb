    "def power_set(arr):\n",
    "    if not arr:\n",
    "        return []\n",
    "    f=arr[0]\n",
    "    rem=power_set(arr[1:])\n",
    "    s=set()\n",
    "    s.add(f)\n",
    "    for e in rem:\n",
    "        s.add(f+e)\n",
    "\n",
    "    return s|set(rem)\n",
    "\n",
    "a=[1,10,100]\n",
    "power_set(a)\n"
   ]
