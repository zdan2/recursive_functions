    "def bunkatsu(a,b):\n",
    "    if b==0:\n",
    "        return 1\n",
    "    if b%2==1:\n",
    "        return a*bunkatsu(a,b//2)*bunkatsu(a,b//2)\n",
    "    else:\n",
    "        return bunkatsu(a,b//2)*bunkatsu(a,b//2)\n",
    "    \n",
    "bunkatsu(2,10)"
