    "class Node:\n",
    "    def __init__(self,val):\n",
    "        self.val=val\n",
    "        self.left=None\n",
    "        self.right=None\n",
    "\n",
    "def make_tree(node,num):\n",
    "        if node==None:\n",
    "             return Node(num)\n",
    "        if num<node.val:\n",
    "            node.left=make_tree(node.left,num)\n",
    "        else:\n",
    "            node.right=make_tree(node.right,num)\n",
    "        return node\n",
    "\n",
    "arr=[random.randint(0,100) for _ in range(20)]\n",
    "print(arr)\n",
    "root=None\n",
